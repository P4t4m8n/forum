{
  "name": "forum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "node lib/database/scripts/createMigration.js",
    "db:truncate": "psql -U postgres -d forum -f lib/database/scripts/seed.sql",
    "db:migrate": "bash migrations/migrate.sh",
    "db:seed": "psql -U postgres -d forum -f lib/database/scripts/seed.sql",
    "db:reset": "npm run db:truncate && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "next": "15.0.2",
    "pg": "^8.13.1",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pg": "^8.11.10",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
